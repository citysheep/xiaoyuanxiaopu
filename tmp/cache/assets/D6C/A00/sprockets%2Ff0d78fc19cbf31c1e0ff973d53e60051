FI"ófunction notify(params){
    var options = { 
        'type' : 'success',
        'message': ''
    }; 
    $.extend(true, options, params);
    var notification = $("div#notification");
    notification.find("div.message").html(options.message);

    notification.addClass(options.type);
    notification.show('fast');    

    notification.find('div.close').click(function(){
       $('div#notification').hide('fast');            
       notification.removeClass(options.type);
    });   
}




