<h2>重新发送帐户解锁的步骤说明</h2>

<%= form_for(resource, :as => resource_name, :url => unlock_path(resource_name), :html => { :method => :post }) do |f| %>

  <div><%= f.label :电子邮箱 %><br />
  <%= f.email_field :email %></div>

  <div><%= f.submit "重发帐户解锁的步骤说明" %></div>
<% end %>

<%= render :partial => "devise/shared/links" %>